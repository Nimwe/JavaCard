<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cda.ListController">
   <children>
      <TableView fx:id="tableView" editable="true" layoutX="52.0" layoutY="133.0" prefHeight="512.0" prefWidth="902.0">
        <columns>
          <TableColumn editable="false" prefWidth="26.0" resizable="false" />
          <TableColumn fx:id="firstName" editable="false" prefWidth="120.0" text="Prénom" />
            <TableColumn fx:id="lastName" editable="false" prefWidth="143.0" text="Nom" />
            <TableColumn fx:id="mobilePhone" editable="false" prefWidth="109.0" text="Téléphone" />
            <TableColumn fx:id="mail" editable="false" prefWidth="143.0" text="E-Mail" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label fx:id="search" alignment="CENTER_RIGHT" layoutX="367.0" layoutY="72.0" prefHeight="30.0" prefWidth="300.0" style="-fx-border-color: #38574C;" text="search" textAlignment="RIGHT" textFill="#38574c">
         <font>
            <Font name="Georgia Bold" size="12.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" centerShape="false" layoutX="52.0" layoutY="646.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="50.0" prefWidth="902.0" spacing="100.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="create" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#create" style="-fx-background-color: none;" text="Créer" textAlignment="CENTER">
                     <font>
                        <Font name="Georgia Bold Italic" size="12.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="update" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#update" style="-fx-background-color: none;" text="Modifier" textAlignment="CENTER">
                     <font>
                        <Font name="Georgia Bold Italic" size="12.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="delete" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#delete" style="-fx-background-color: none;" text="Supprimer" textAlignment="CENTER">
                     <font>
                        <Font name="Georgia Bold Italic" size="12.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="export" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#export" style="-fx-background-color: none;" text="Exporter" textAlignment="CENTER">
                     <font>
                        <Font name="Georgia Bold Italic" size="12.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="cancel" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#delete" style="-fx-background-color: none;" text="Annuler" textAlignment="CENTER">
                     <font>
                        <Font name="Georgia Bold Italic" size="12.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</Pane>
